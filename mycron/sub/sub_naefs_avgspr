set -x

Date=`date +%y%m%d%H`
CDATE=20$Date

dir=/lfs/h2/emc/vpppg/noscrub/$LOGNAME/naefs.v7.0.0/mycron

job=sub_jnaefs_prob_avgspr.ecf

export YYMMDD=`echo $CDATE | cut -c1-8`
export CYC=00                               
echo " day " $YYMMDD$cyc

rm $dir/output/$job.temp

sed -e "s/YYMMDD/$YYMMDD/" \
    -e "s/CYC/$CYC/" \
    $dir/ecf/$job > $dir/output/$job.temp

cd $dir/output
qsub $dir/output/$job.temp
