#filelist="sub_cmce_bias"  

export REPLACE=/u/$LOGNAME/xbin/replace.pl

mkdir dir.org

for file in $filelist; do
  echo $file
  cp $file $file.org
done

for file in $filelist; do
  echo $file
# $REPLACE $file.org $file.v1 hps\/emc    hps3\/emc             
# $REPLACE $file.v1  $file.v2 hps\/ptmp   hps3\/ptmp            
# $REPLACE $file.v2  $file.v3 hps\/stmp   hps3\/stmp            

  $REPLACE $file.org $file.v1 gpfs\/hps3\/emc\/ensemble\/save\/emc.enspara1\/bc\/naefs.v6.0.0\/ecf lfs\/h2\/emc\/vpppg\/noscrub\/$LOGNAME\/naefs.v7.0.0\/mycron\/ecf
  cp $file.v1 $file
  rm $file.v*
  chmod +x $file
  diff $file $file.org > diff_$file
  mv $file.org dir.org 
done
