
DATA=/lfs/h2/emc/ptmp/bo.cui/git
dir=naefs.v7.0.0

cd $DATA
rm -rf $DATA/$dir
git clone git@github.com:BoCui-NOAA/NAEFS.git $dir              
cd $dir            
git checkout naefs.v7.0.0_test

COM=/lfs/h2/emc/vpppg/noscrub/bo.cui

subdirlist="ecf          scripts run_check_para_v7      jobs docs ush versions grads "
subdirlist="ecf ecf_prod scripts run_check_para_v7 sorc jobs docs ush versions grads "
subdirlist="mycron arch docs ecf ecf_prod gempak grads jobs modulefiles run_check_grads_old run_check_para_v7 save_docs scripts sorc tools ush verfication versions"

subdirlist="mycron verification"
subdirlist="docs jobs sorc mycron ecf verification tools"                             

# sync dogwood from cactus

for subdir in $subdirlist; do
  cp -rp $COM/$dir/$subdir .
done

rm $DATA/$dir/mycron/output/*naefs*
rm $DATA/$dir/mycron/output/*out*
rm $DATA/$dir/mycron/backup/*naefs*
rm $DATA/$dir/verification/output/*
rm $DATA/$dir/verification/fix/*
rm $DATA/$dir/ecf/*/*/*/*.o*       
rm $DATA/$dir/ecf/*/*/*.o*       
rm $DATA/$dir/ecf/*/*.o*       
