
COM1=/gpfs/hps3/emc/ensemble/save/emc.enspara1/bc/naefs.v6.0.0
COM2=/gpfs/hps/nco/ops/nwpara/naefs.v6.0.2

filelist="exnaefs_dvrtma_prob_avgspr_ak.sh.ecf exnaefs_dvrtma_prob_avgspr_conus.sh.ecf"

