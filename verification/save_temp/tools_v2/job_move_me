CDATE=2016080200
ndays=30
iday=1

COM=$NGLOBAL/Bo.Cui/naefs.v6.0.1/com/gens/prod          
COMO=$NGLOBAL/Bo.Cui/naefs.v6.0.2/com/gens/prod


while [ $iday -le $ndays ]; do

  PDY=`echo $CDATE | cut -c1-8`
  cyc=`echo $CDATE | cut -c9-10`

  echo " day " $PDY$cyc

  mkdir -p $COMO/gefs.$PDY/$cyc
  COMIN=$COM/gefs.$PDY/$cyc/pgrb2ap5_gen
  COMOUT=$COMO/gefs.$PDY/$cyc/pgrb2ap5
  mv $COMIN $COMOUT

  iday=`expr $iday + 1`
  CDATE=`$NDATE +24 $CDATE`

done


