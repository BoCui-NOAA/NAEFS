# run_anl_3centers_para_2p5 $CDATE > $PTMP/Bo.Cui/trash 2>&1&
  bsub < run_vefy.ecf

  iday=`expr $iday + 1`
  export CDATE=`$NDATE +24 $CDATE`

done

