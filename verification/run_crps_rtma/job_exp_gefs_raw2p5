iday=1
 
while [ $iday -le $ndays ]; do

  export id=gefs
  export ireg=conus      
  export IF_NDGD=YES

  export DSIM=gefs_raw2p5
# export ENSIM=gefs_p5raw2p5

  export exid=_raw2p5
