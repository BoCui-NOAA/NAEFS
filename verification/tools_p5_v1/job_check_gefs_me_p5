CDATE=2016020100
ndays=31 
iday=1

COM=$NGLOBAL/Bo.Cui/naefs.v6.0.4/com/gens/prod          

while [ $iday -le $ndays ]; do

  PDY=`echo $CDATE | cut -c1-8`
  cyc=`echo $CDATE | cut -c9-10`

  echo " day " $PDY$cyc

  COMINNCEP=$COM/gefs.$PDY/$cyc
  ls $COMINNCEP/pgrb2ap5/*geavg*me* | wc

  iday=`expr $iday + 1`
  CDATE=`$NDATE +24 $CDATE`

done


